	public ConnectTask(TCPClient myClient, ServerResponse serverResponse){
		this.myTClient=myClient;
		this.serverResponse=serverResponse;
	}
	
	@Override
	protected TCPClient doInBackground(ServerResponse... params) {
		
		return null;
	}
	
}
